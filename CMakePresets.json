{
  "version": 3,
  "configurePresets": [
    {
      "name": "arm64",
      "description": "Build in termux for Android arm64",
      "generator": "Ninja",
      "cacheVariables": {
        "ANDROID_ABI": "arm64-v8a",
        "ANDROID_PLATFORM": "android-34",
        "ANDROID_STL": "none",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
	    "CMAKE_TOOLCHAIN_FILE": "$env{ANDROID_NDK}/build/cmake/android.toolchain.cmake"
      }
    }
  ]
}
